# Polymarket Trading Configuration
# Copy this file to .env and fill in your actual credentials

# Your proxy wallet's private key (without 0x prefix)
POLYMARKET_PRIVATE_KEY=your_private_key_here

# Your main wallet's public address (with 0x prefix)
POLYMARKET_FUNDER=0xYourFunderAddressHere

# Optional: Chain ID (default is 137 for Polygon mainnet)
# CHAIN_ID=137

# Optional: Enable verbose logging
# VERBOSE=true

# Optional: Enable dry run mode (for testing without real trades)
# DRY_RUN=true

# Opinion
OPINIOON_API_KEY=your_api_key_here
OPINIOON_RPC_URL=https://bsc-dataseed.binance.org
OPINIOON_PRIVATE_KEY=0x1234567890abcdef...
OPINIOON_MULTI_SIG_ADDR=0xYourWalletAddress...
OPINIOON_HOST=https://proxy.opinion.trade:8443
OPINIOON_CHAIN_ID=56
OPINIOON_CONDITIONAL_TOKEN_ADDR=0xAD1a38cEc043e70E83a3eC30443dB285ED10D774
OPINIOON_MULTISEND_ADDR=0x998739BFdAAdde7C933B942a68053933098f9EDa